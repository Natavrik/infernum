/* программа с помощью оператора for 
выводит последовательость  чисел*/

#include <stdio.h>
int main()
{
  int i;
  for(i=1;i<8;i++)
    if(i!=7) printf("%d,",i);
    else printf("%d\n",i);
  for(i=3;i<24;i+=5)
    if(i!=23) printf("%d,",i);
    else printf("%d\n",i);
  for(i=20;i>-11;i-=6)
    if(i!=-10) printf("%d,",i);
    else printf("%d\n",i);
  for(i=19;i<52;i+=8)
    if(i!=51) printf("%d,",i);
    else printf("%d\n",i);
    return 0;
}

